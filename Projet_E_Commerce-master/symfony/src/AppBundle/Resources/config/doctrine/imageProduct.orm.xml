<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\imageProductRepository" name="AppBundle\Entity\imageProduct">
    <id name="id_image" type="integer" column="id_image">
      <generator strategy="AUTO"/>
    </id>
    <field name="image" type="text" column="image"/>
    <field name="idProduct" type="integer" column="id_product"/>
  </entity>
</doctrine-mapping>
